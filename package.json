{
  "name": "dennydap.github.io",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://dennypradipta.github.io",
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "gh-pages": "^2.1.1",
    "moment-timezone": "^0.5.27",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.2",
    "react": "^16.12.0",
    "react-bulma-components": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "svg-inline-loader": "^0.8.0"
  },
  "scripts": {
    "predeploy": "npm run format && npm run check-format && npm run build",
    "deploy": "gh-pages -b master -d build",
    "check-format": "prettier --check \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
